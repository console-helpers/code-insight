{
	"name": "console-helpers/code-insight",
	"description": "A tool for analysing other project code bases",
	"license": "BSD-3-Clause",
	"require": {
		"php": ">=5.6.0",
		"console-helpers/console-kit": "^0.1",
		"symfony/finder": "^3.4",
		"goaop/parser-reflection": "^1.4",
		"console-helpers/db-migration": "^0.1.0",
		"aura/sql": "^2.5",
		"doctrine/cache": "^1.5",
		"camspiers/json-pretty": "^1.0"
	},
	"require-dev": {
		"aik099/coding-standard": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"ConsoleHelpers\\CodeInsight\\": "src/CodeInsight/"
		},
		"files": ["src/ReflectionType.php"]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\ConsoleHelpers\\CodeInsight\\": "tests/CodeInsight/"
		}
	},
	"bin": [
		"bin/code-insight"
	]
}
